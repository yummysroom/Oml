local LocalPlayer = game.Players.LocalPlayer
local Allowed = {1289612805; 766173417;} --UserID instead so you are not gonna do some hardwork to spell their names.
local function allowedPeople(thisPlayer)
    for _, playerID in pairs(Allowed) do
        if playerID == thisPlayer.UserId then
            return true
        end
    end
end

if allowedPeople(LocalPlayer) == true then
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/yummysroom/Oml/main/no"),true))()
    else
        print("join discord to use this script")
end
