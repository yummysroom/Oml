local LocalPlayer = game.Players.LocalPlayer
local Allowed = {1289612805; 766173417;} --UserID
local function allowedPeople(thisPlayer)
    for _, playerID in pairs(Allowed) do
        if playerID == thisPlayer.UserId then
            return true
        end
    end
end

if allowedPeople(LocalPlayer) == true then
    loadstring(game:HttpGet(("https://raw.githubusercontent.com/yummysroom/Oml/main/!Gems"),true))()
    else
        print("join discord to get acces --https://discord.gg/nWctUBtH--")
end
