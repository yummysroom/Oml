local UILibrary = loadstring(game:HttpGet("https://pastebin.com/raw/V1ca2q9s"))()
local MainUI = UILibrary.Load("Sciencetits")
local FirstPage = MainUI.AddPage("Esp")
local FirstLabel = FirstPage.AddLabel("Super Nice Esp v")
local FirstButton = FirstPage.AddButton("Wxer Esp", function()
WxerSoftworks = 'https://discord.gg/d8Jc9ggzJU'
loadstring(game:HttpGet('https://raw.githubusercontent.com/Wxer/roblox/main/random_scripts/entrypoint.lua'))()
end)

local FirstButton = FirstPage.AddButton("Homemade esp (cant find out how to use onChildAdded", function()
	 --PowerBox Esp
	 for i,v in pairs(game.Workspace.Level.Geometry:GetChildren()) do
                    if v.Name == "PowerBox" then
                        if v.Case.BasePart:FindFirstChild("localoca") then
                        else
                            local Pesp = Instance.new('Highlight') 
    	
    	                    Pesp.Parent = v
                    	    Pesp.Name = "pesp"
    	
                    	   
                        end
                    end
end
--Npc Esp
for i,v in pairs(game.Workspace.Level.Actors:GetChildren()) do
local Nppc = Instance.new("Highlight")

Nppc.Name = "HeadN"
Nppc.Parent = v
end

local folder = game.Workspace.Level.Actors

local function onChildAdded(child)
	local highlight = Instance.new("Highlight")
	highlight.Name = "HeadN"
	highlight.Parent = child
end

folder.ChildAdded:Connect(onChildAdded)


--Computer Esp
for i,v in pairs(game.Workspace.Level.Geometry:GetChildren()) do
            local oml = 100
            if v.Name == "Computer" then
            if oml == 1 then
            else
            local Cesp = Instance.new("Highlight")
            
            Cesp.Parent = v 
            Cesp.Name = "Cesp"
        end
    end
end

--Cam Esp
for i,v in pairs(game.Workspace.Level.Glass:GetChildren()) do
            local oml = 100
            if v.Name == "ExteriorCam" then
            if oml == 1 then
            else
            local Caesp = Instance.new("Highlight")
            
            Caesp.Parent = v 
            Caesp.Name = "Caesp"
        end
    end
end

end)

local FirstPage = MainUI.AddPage("Teleports")

local FirstLabel = FirstPage.AddLabel("Yet another label")
local FirstDropdown = FirstPage.AddDropdown("Teleports", {
"Screwdriver",
"Cutter",
"Vent",
"Control Room"
}, function(Value)
if Value == "Screwdriver" then
    	        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	            wait(0.1)
	            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Level.GroundItems.Screwdriver.Base.CFrame
	        elseif Value == "Cutter" then
    	        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	            wait(0.1)
	            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Level.GroundItems.Cutter.Base.CFrame
    	    elseif Value == "Vent" then
    	        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	            wait(0.1)
	            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-26.6, -6.4, 3.6)
    	    elseif Value == "Control Room" then
    	        game.Players.LocalPlayer.Character.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping)
	            wait(0.1)
	            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-19, 17, 21)
    	    end
end)



